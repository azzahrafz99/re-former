<h1>User Page </h1>

  <%= link_to 'Create User', new_user_path %>
  <%= link_to 'Create Post', new_post_path %>
  <%= link_to 'Logout', logout_path %>


<table>
  <tr>
    <th>username</th>
    <th>email</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
     <td> <%= user.username %> </td>
     <td> <%= user.email %> </td>
     <td><%= link_to 'edit',edit_user_path(user)%></td>
    </tr>
  <% end %>

</table>
